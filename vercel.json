{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "Server/src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "Server/src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Server/src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}